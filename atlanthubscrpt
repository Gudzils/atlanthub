-- Aimbot Settings
local teamCheck = false
local fov = 150  -- Default FOV value

local RunService = game:GetService("RunService")

local FOVring = Drawing.new("Circle")
FOVring.Visible = true
FOVring.Thickness = 1.5
FOVring.Radius = fov
FOVring.Transparency = 1
FOVring.Color = Color3.fromRGB(255, 128, 128)
FOVring.Position = workspace.CurrentCamera.ViewportSize/2

-- Aimbot main loop
local function getClosest(cframe)
    local ray = Ray.new(cframe.Position, cframe.LookVector).Unit
    local target = nil
    local mag = math.huge
    
    for _, v in pairs(workspace:GetDescendants()) do
        if v:IsA("Model") and v:FindFirstChild("Head") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v ~= game.Players.LocalPlayer.Character then
            local magBuf = (v.Head.Position - ray:ClosestPoint(v.Head.Position)).Magnitude
            if magBuf < mag then
                mag = magBuf
                target = v
            end
        end
    end
    
    return target
end

-- FullBright (Always Day)
CombatTab:CreateButton({
    Name = "FullBright (Always Day)",
    Callback = function()
        game.Lighting.TimeOfDay = "14:00:00" -- Set time to always be day
        game.Lighting.FogEnd = 100000
        game.Lighting.Brightness = 2
        game.Lighting.OutdoorAmbient = Color3.fromRGB(255, 255, 255) -- Always bright
    end
})

-- Aimbot toggle
CombatTab:CreateToggle({
    Name = "Enable Aimbot",
    CurrentValue = false,
    Flag = "AimbotToggle",
    Callback = function(value)
        if value then
            -- Aimbot activation
            RunService.RenderStepped:Connect(function()
                local UserInputService = game:GetService("UserInputService")
                local pressed = UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2)  -- Right mouse button to activate Aimbot
                local localPlayer = game.Players.LocalPlayer
                local cam = workspace.CurrentCamera
                local zz = workspace.CurrentCamera.ViewportSize/2

                if pressed then
                    local curTar = getClosest(cam.CFrame)
                    if curTar then
                        local ssHeadPoint = cam:WorldToScreenPoint(curTar.Head.Position)
                        ssHeadPoint = Vector2.new(ssHeadPoint.X, ssHeadPoint.Y)
                        if (ssHeadPoint - zz).Magnitude < fov then
                            cam.CFrame = cam.CFrame:Lerp(CFrame.new(cam.CFrame.Position, curTar.Head.Position), 0.1)
                        end
                    end
                end
            end)
        else
            -- Aimbot deactivation
            print("Aimbot disabled.")
        end
    end
})

-- Flyv3 (Fly Script)
CombatTab:CreateButton({
    Name = "Enable Flyv3",
    Callback = function()
        local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/Gudzils/vflybysiwkav/refs/heads/main/vfly.raw'))()
        Rayfield()
    end
})

-- ESP Tab for Models
local EspTab = Window:CreateTab("ESP", 4483362458)

local espEnabled = false
local highlights = {}
local distanceLabels = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è ESP –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤
local function AutoSet(player)
    if player == game.Players.LocalPlayer then return end

    local function AutoSetupCharacter(character)
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart", 10)

        -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ highlight
        local highlight = Instance.new("Highlight")
        highlight.Parent = character
        highlight.Adornee = character
        highlight.FillColor = Color3.fromRGB(255, 0, 0)
        highlight.FillTransparency = 0.5
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.OutlineTransparency = 0
        highlights[player] = highlight

        -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –º–µ—Ç–∫–∏ —Å —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ–º
        local billboard = Instance.new("BillboardGui")
        billboard.Size = UDim2.new(0, 150, 0, 30)
        billboard.Adornee = humanoidRootPart
        billboard.StudsOffset = Vector3.new(0, 2, 0)
        billboard.AlwaysOnTop = true
        billboard.Parent = humanoidRootPart

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(1, 0, 1, 0)
        textLabel.BackgroundTransparency = 1
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        textLabel.TextScaled = true
        textLabel.Font = Enum.Font.SourceSansBold
        textLabel.Text = player.Name  -- –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –∏–º–µ–Ω–∏ –∏–≥—Ä–æ–∫–∞
        textLabel.Parent = billboard
    end

    if player.Character then
        AutoSetupCharacter(player.Character)
    end

    player.CharacterAdded:Connect(function(character)
        AutoSetupCharacter(character)
    end)
end

-- –£–¥–∞–ª–µ–Ω–∏–µ ESP –∏–≥—Ä–æ–∫–æ–≤, –∫–æ–≥–¥–∞ –∏–≥—Ä–æ–∫ –ø–æ–∫–∏–¥–∞–µ—Ç –∏–≥—Ä—É
game.Players.PlayerRemoving:Connect(function(player)
    if highlights[player] then
        highlights[player]:Destroy()
        highlights[player] = nil
    end
end)

-- –ü—Ä–∏–º–µ–Ω–µ–Ω–∏–µ ESP –∫ —Å—É—â–µ—Å—Ç–≤—É—é—â–∏–º –∏–≥—Ä–æ–∫–∞–º
for _, player in pairs(game.Players:GetPlayers()) do
    AutoSet(player)
end

-- –ü—Ä–∏–º–µ–Ω–µ–Ω–∏–µ ESP –∫ –Ω–æ–≤—ã–º –∏–≥—Ä–æ–∫–∞–º
game.Players.PlayerAdded:Connect(AutoSet)

-- ESP toggle –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤
EspTab:CreateToggle({
    Name = "Enable ESP for Players",
    CurrentValue = false,
    Flag = "PlayerEspToggle",
    Callback = function(value)
        espEnabled = value
        if espEnabled then
            -- –í–∫–ª—é—á–µ–Ω–∏–µ ESP –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤
            for _, player in pairs(game.Players:GetPlayers()) do
                AutoSet(player)
            end
        else
            -- –û—Ç–∫–ª—é—á–µ–Ω–∏–µ ESP –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤
            for player, highlight in pairs(highlights) do
                if highlight then
                    highlight:Destroy()
                    highlights[player] = nil
                end
            end
        end
    end
})

-- ESP –¥–ª—è –º–æ–¥–µ–ª–µ–π
local function AutoSetModel(model)
    if model == game.Players.LocalPlayer then return end
    if not model:IsA("Model") then return end  -- –¢–æ–ª—å–∫–æ –¥–ª—è –º–æ–¥–µ–ª–µ–π

    local function AutoSetupModel(model)
        -- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ highlight
        local highlight = Instance.new("Highlight")
        highlight.Parent = model
        highlight.Adornee = model
        highlight.FillColor = Color3.fromRGB(255, 0, 0)
        highlight.FillTransparency = 0.5
        highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
        highlight.OutlineTransparency = 0
        highlights[model] = highlight
    end

    AutoSetupModel(model)

    model.DescendantAdded:Connect(function(child)
        if child:IsA("Model") then
            AutoSetupModel(child)
        end
    end)
end

-- –£–¥–∞–ª–µ–Ω–∏–µ ESP –º–æ–¥–µ–ª–µ–π, –∫–æ–≥–¥–∞ –º–æ–¥–µ–ª—å –∏—Å—á–µ–∑–∞–µ—Ç
game:GetService("RunService").Heartbeat:Connect(function()
    for _, model in pairs(workspace:GetChildren()) do
        if model:IsA("Model") and not highlights[model] then
            AutoSetModel(model)
        end
    end
end)

-- ESP toggle –¥–ª—è –º–æ–¥–µ–ª–µ–π
EspTab:CreateToggle({
    Name = "Enable ESP for Models",
    CurrentValue = false,
    Flag = "ModelEspToggle",
    Callback = function(value)
        espEnabled = value
        if espEnabled then
            -- –í–∫–ª—é—á–µ–Ω–∏–µ ESP –¥–ª—è –º–æ–¥–µ–ª–µ–π
            for _, model in pairs(workspace:GetChildren()) do
                if model:IsA("Model") then
                    AutoSetModel(model)
                end
            end
        else
            -- –û—Ç–∫–ª—é—á–µ–Ω–∏–µ ESP –¥–ª—è –º–æ–¥–µ–ª–µ–π
            for model, highlight in pairs(highlights) do
                if highlight then
                    highlight:Destroy()
                    highlights[model] = nil
                end
            end
        end
    end
})

Rayfield:Init()

