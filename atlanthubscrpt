do local Rayfield=loadstring(game:HttpGet("https://sirius.menu/rayfield"))();local Window=Rayfield:CreateWindow({Name="Atlant hubрџЋЉ",LoadingTitle="рџЋ­Dead ralis",LoadingSubtitle="by @Fiagoo",ConfigurationSaving={Enabled=true,FileName="Big Hub"},KeySystem=false});local PlayerTab=Window:CreateTab("Set Money",4483362458);PlayerTab:CreateSlider({Name="рџЋ‰Set Money",Range={1,9999},Increment=1,Suffix="Coins",CurrentValue=1,Flag="MoneySlider",Callback=function(value) local player=game.Players.LocalPlayer;local playerGui=player:WaitForChild("PlayerGui");local moneyGui=playerGui:FindFirstChild("MoneyGui");if moneyGui then local money=moneyGui:FindFirstChild("Money");if money then money.Text=tostring(value);end end end});PlayerTab:CreateSlider({Name="рџ’ёSet Bond",Range={1,9999},Increment=1,Suffix="Bonds",CurrentValue=1,Flag="BondCountSlider",Callback=function(value) local player=game.Players.LocalPlayer;local playerGui=player:WaitForChild("PlayerGui");local bondGui=playerGui:FindFirstChild("BondGui");if bondGui then local bondInfo=bondGui:FindFirstChild("BondInfo");if bondInfo then local bondCount=bondInfo:FindFirstChild("BondCount");if bondCount then bondCount.Text=tostring(value);end end end end});PlayerTab:CreateButton({Name="Telegram Link",Callback=function() setclipboard("https://t.me/atlanthub");end});local AntiCheatTab=Window:CreateTab("Off Anti Cheat",4483362458);AntiCheatTab:CreateToggle({Name="Disable Anti-Cheat Scripts",CurrentValue=false,Flag="AntiCheatToggle",Callback=function(value) if value then local player=game.Players.LocalPlayer;local antiScripts={game:FindFirstChild("AntiFling"),game:FindFirstChild("AntiSlide"),game:FindFirstChild("AntiSlideClientTest")};for _,script in pairs(antiScripts) do if script then script:Destroy();print("Anti-Cheat script removed: "   .. script.Name );end end else print("Anti-Cheat scripts remain disabled.");end end});local CombatTab=Window:CreateTab("Combat",4483362458);CombatTab:CreateSlider({Name="Walk Speed",Range={16,500},Increment=1,Suffix="Speed",CurrentValue=16,Flag="WalkSpeedSlider",Callback=function(value) local player=game.Players.LocalPlayer;if player.Character then local humanoid=player.Character:FindFirstChild("Humanoid");if humanoid then humanoid.WalkSpeed=value;end end end});CombatTab:CreateSlider({Name="Jump Power",Range={50,300},Increment=1,Suffix="Jump Power",CurrentValue=50,Flag="JumpPowerSlider",Callback=function(value) local player=game.Players.LocalPlayer;if player.Character then local humanoid=player.Character:FindFirstChild("Humanoid");if humanoid then humanoid.JumpPower=value;end end end});local teamCheck=false;local fov=150;local RunService=game:GetService("RunService");local FOVring=Drawing.new("Circle");FOVring.Visible=true;FOVring.Thickness=1.5;FOVring.Radius=fov;FOVring.Transparency=1;FOVring.Color=Color3.fromRGB(255,128,128);FOVring.Position=workspace.CurrentCamera.ViewportSize/2 ;local function getClosest(cframe) local ray=Ray.new(cframe.Position,cframe.LookVector).Unit;local target=nil;local mag=math.huge;for _,v in pairs(workspace:GetDescendants()) do if (v:IsA("Model") and v:FindFirstChild("Head") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and (v~=game.Players.LocalPlayer.Character)) then local magBuf=(v.Head.Position-ray:ClosestPoint(v.Head.Position)).Magnitude;if (magBuf<mag) then mag=magBuf;target=v;end end end return target;end CombatTab:CreateButton({Name="FullBright (Always Day)",Callback=function() game.Lighting.TimeOfDay="14:00:00";game.Lighting.FogEnd=100000;game.Lighting.Brightness=2;game.Lighting.OutdoorAmbient=Color3.fromRGB(255,255,255);end});CombatTab:CreateToggle({Name="Enable Aimbot",CurrentValue=false,Flag="AimbotToggle",Callback=function(value) if value then RunService.RenderStepped:Connect(function() local UserInputService=game:GetService("UserInputService");local pressed=UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2);local localPlayer=game.Players.LocalPlayer;local cam=workspace.CurrentCamera;local zz=workspace.CurrentCamera.ViewportSize/2 ;if pressed then local curTar=getClosest(cam.CFrame);if curTar then local ssHeadPoint=cam:WorldToScreenPoint(curTar.Head.Position);ssHeadPoint=Vector2.new(ssHeadPoint.X,ssHeadPoint.Y);if ((ssHeadPoint-zz).Magnitude<fov) then cam.CFrame=cam.CFrame:Lerp(CFrame.new(cam.CFrame.Position,curTar.Head.Position),0.1);end end end end);else print("Aimbot disabled.");end end});CombatTab:CreateButton({Name="Enable Flyv3",Callback=function() local Rayfield=loadstring(game:HttpGet("https://raw.githubusercontent.com/Gudzils/vflybysiwkav/refs/heads/main/vfly.raw"))();Rayfield();end});local EspTab=Window:CreateTab("ESP",4483362458);local espEnabled=false;local highlights={};local distanceLabels={};local function AutoSet(player) if (player==game.Players.LocalPlayer) then return;end local function AutoSetupCharacter(character) local humanoidRootPart=character:WaitForChild("HumanoidRootPart",10);local highlight=Instance.new("Highlight");highlight.Parent=character;highlight.Adornee=character;highlight.FillColor=Color3.fromRGB(255,0,0);highlight.FillTransparency=0.5;highlight.OutlineColor=Color3.fromRGB(255,255,255);highlight.OutlineTransparency=0;highlights[player]=highlight;local billboard=Instance.new("BillboardGui");billboard.Size=UDim2.new(0,150,0,30);billboard.Adornee=humanoidRootPart;billboard.StudsOffset=Vector3.new(0,2,0);billboard.AlwaysOnTop=true;billboard.Parent=humanoidRootPart;local textLabel=Instance.new("TextLabel");textLabel.Size=UDim2.new(1,0,1,0);textLabel.BackgroundTransparency=1;textLabel.TextColor3=Color3.fromRGB(255,255,255);textLabel.TextScaled=true;textLabel.Font=Enum.Font.SourceSansBold;textLabel.Text=player.Name;textLabel.Parent=billboard;end if player.Character then AutoSetupCharacter(player.Character);end player.CharacterAdded:Connect(function(character) AutoSetupCharacter(character);end);end game.Players.PlayerRemoving:Connect(function(player) if highlights[player] then highlights[player]:Destroy();highlights[player]=nil;end end);for _,player in pairs(game.Players:GetPlayers()) do AutoSet(player);end game.Players.PlayerAdded:Connect(AutoSet);EspTab:CreateToggle({Name="Enable ESP for Players",CurrentValue=false,Flag="PlayerEspToggle",Callback=function(value) espEnabled=value;if espEnabled then for _,player in pairs(game.Players:GetPlayers()) do AutoSet(player);end else for player,highlight in pairs(highlights) do if highlight then highlight:Destroy();highlights[player]=nil;end end end end});local function AutoSetModel(model) if (model==game.Players.LocalPlayer) then return;end if  not model:IsA("Model") then return;end local function AutoSetupModel(model) local highlight=Instance.new("Highlight");highlight.Parent=model;highlight.Adornee=model;highlight.FillColor=Color3.fromRGB(255,0,0);highlight.FillTransparency=0.5;highlight.OutlineColor=Color3.fromRGB(255,255,255);highlight.OutlineTransparency=0;highlights[model]=highlight;end AutoSetupModel(model);model.DescendantAdded:Connect(function(child) if child:IsA("Model") then AutoSetupModel(child);end end);end game:GetService("RunService").Heartbeat:Connect(function() for _,model in pairs(workspace:GetChildren()) do if (model:IsA("Model") and  not highlights[model]) then AutoSetModel(model);end end end);EspTab:CreateToggle({Name="Enable ESP for Models",CurrentValue=false,Flag="ModelEspToggle",Callback=function(value) espEnabled=value;if espEnabled then for _,model in pairs(workspace:GetChildren()) do if model:IsA("Model") then AutoSetModel(model);end end else for model,highlight in pairs(highlights) do if highlight then highlight:Destroy();highlights[model]=nil;end end end end});Rayfield:Init(); end
