local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "Atlant hubüéä",
    LoadingTitle = "üé≠Dead ralis",
    LoadingSubtitle = "by @Fiagoo",
    ConfigurationSaving = {
        Enabled = true,
        FileName = "Big Hub"
    },
    KeySystem = false,
})

-- Set Money Tab
local PlayerTab = Window:CreateTab("Set Money", 4483362458)

-- Set Money Slider
PlayerTab:CreateSlider({
    Name = "üéâSet Money",
    Range = {1, 9999},
    Increment = 1,
    Suffix = "Coins",
    CurrentValue = 1,
    Flag = "MoneySlider",
    Callback = function(value)
        local player = game.Players.LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")
        local moneyGui = playerGui:FindFirstChild("MoneyGui")
        if moneyGui then
            local money = moneyGui:FindFirstChild("Money")
            if money then
                money.Text = tostring(value)
            end
        end
    end
})

-- Set Bond Slider
PlayerTab:CreateSlider({
    Name = "üí∏Set Bond",
    Range = {1, 9999},
    Increment = 1,
    Suffix = "Bonds",
    CurrentValue = 1,
    Flag = "BondCountSlider",
    Callback = function(value)
        local player = game.Players.LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")
        local bondGui = playerGui:FindFirstChild("BondGui")
        if bondGui then
            local bondInfo = bondGui:FindFirstChild("BondInfo")
            if bondInfo then
                local bondCount = bondInfo:FindFirstChild("BondCount")
                if bondCount then
                    bondCount.Text = tostring(value)
                end
            end
        end
    end
})

