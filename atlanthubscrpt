-- Anti-Cheat Tab
local AntiCheatTab = Window:CreateTab("Off Anti Cheat", 4483362458)

AntiCheatTab:CreateToggle({
    Name = "Disable Anti-Cheat Scripts",
    CurrentValue = false,
    Flag = "AntiCheatToggle",
    Callback = function(value)
        if value then
            -- –£–¥–∞–ª—è–µ–º –∞–Ω—Ç–∏—á–∏—Ç —Å–∫—Ä–∏–ø—Ç—ã
            local player = game.Players.LocalPlayer

            -- –ü—Ä–∏–º–µ—Ä –≤–æ–∑–º–æ–∂–Ω—ã—Ö –∞–Ω—Ç–∏—á–∏—Ç-—Å–∫—Ä–∏–ø—Ç–æ–≤, –∫–æ—Ç–æ—Ä—ã–µ –º–æ–≥—É—Ç –±—ã—Ç—å –≤ –∏–≥—Ä–µ:
            local antiScripts = {
                game:FindFirstChild("AntiFling"),
                game:FindFirstChild("AntiSlide"),
                game:FindFirstChild("AntiSlideClientTest")
            }

            -- –ü–µ—Ä–µ–±–æ—Ä —Å–∫—Ä–∏–ø—Ç–æ–≤ –∏ —É–¥–∞–ª–µ–Ω–∏–µ
            for _, script in pairs(antiScripts) do
                if script then
                    script:Destroy()
                    print("Anti-Cheat script removed: " .. script.Name)
                end
            end
        else
            -- –õ–æ–≥–∏–∫–∞, –µ—Å–ª–∏ –∞–Ω—Ç–∏—á–∏—Ç —Å–∫—Ä–∏–ø—Ç—ã –Ω–µ –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å —É–¥–∞–ª–µ–Ω—ã
            print("Anti-Cheat scripts remain disabled.")
        end
    end
})

